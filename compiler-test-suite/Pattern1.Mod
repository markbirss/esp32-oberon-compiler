MODULE Pattern1;
  IMPORT Out;
IMPORT SYSTEM;
  VAR ch: CHAR;
      k,j: INTEGER;
      x: REAL;
      s: SET;
BEGIN
  ch := "0";
  Out.Int(ORD(ch));
  k := 10;
  Out.Int(k);
  k := -65536;
  Out.Int(k);
  k := -65537;
  Out.Int(k);
  k := 65535;
  Out.Int(k);
  k := 65536;
  Out.Int(k);
  k := 65537;
  Out.Int(k);
  x := 1.0;
  Out.Real(x);
  s := {0, 4, 8};
  Out.Int(SYSTEM.VAL(INTEGER, s));
  s := {3..5};
  Out.Int(SYSTEM.VAL(INTEGER, s));
  k := 5;
  s := {3..k};
  Out.Int(SYSTEM.VAL(INTEGER, s));
  k := 18;
  s := {16..k};
  Out.Int(SYSTEM.VAL(INTEGER, s));
  k := 3;
  s := {k..5};
  Out.Int(SYSTEM.VAL(INTEGER, s));
  k := 3;
  s := {k..15};
  Out.Int(SYSTEM.VAL(INTEGER, s));
  j := 5;
  s := {k..j};
  Out.Int(SYSTEM.VAL(INTEGER, s));
END Pattern1.