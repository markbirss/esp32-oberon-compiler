MODULE Pattern2c;
  IMPORT Out, SYSTEM;
  VAR s, t, u: SET;
BEGIN 
  s := {1, 5} + {2, 7, 4};
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  s := {1, 5} * {7, 1, 2};
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  s := {1, 5} - {7, 1, 2};
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  s := {1, 5, 2} / {7, 1, 2};
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  u := {1, 5};
  s := u + {2, 7, 4};
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  u := {1, 5};
  s := u * {7, 1, 2};
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  u := {1, 5};
  s := u - {7, 1, 2};
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  u := {1, 5, 2};
  s := u / {7, 1, 2};
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  u := {1, 5};
  t := {2, 7, 4};
  s := u + t;
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  u := {1, 5};
  t := {7, 1, 2};
  s := u * t;
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  u := {1, 5};
  t := {7, 1, 2};
  s := u - t;
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  u := {1, 5, 2};
  t := {7, 1, 2};
  s := u / t;
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
END Pattern2c.