MODULE VarInit;
  IMPORT Out, SYSTEM;
  TYPE R = RECORD x : INTEGER END;
  VAR  
    x0 : INTEGER;
    x1 : BOOLEAN;
    x2 : SET;
    x3 : BYTE;
    x4 : REAL;
    x5 : CHAR;
    x6 : R;
    y0 : ARRAY 1 OF INTEGER;
    y1 : ARRAY 1 OF BOOLEAN;
    y2 : ARRAY 1 OF SET;
    y3 : ARRAY 1 OF BYTE;
    y4 : ARRAY 1 OF REAL;
    y5 : ARRAY 1 OF CHAR; 
    y6 : ARRAY 1 OF R; 
  
  PROCEDURE P;
    VAR  
      x0 : INTEGER;
      x1 : BOOLEAN;
      x2 : SET;
      x3 : BYTE;
      x4 : REAL;
      x5 : CHAR;
      x6 : R;
      y0 : ARRAY 1 OF INTEGER;
      y1 : ARRAY 1 OF BOOLEAN;
      y2 : ARRAY 1 OF SET;
      y3 : ARRAY 1 OF BYTE;
      y4 : ARRAY 1 OF REAL;
      y5 : ARRAY 1 OF CHAR; 
      y6 : ARRAY 1 OF R; 
  BEGIN
    Out.Int(x0, 10);
    IF ~x1 THEN  Out.Int(0, 10) END;
    Out.Int(SYSTEM.VAL(INTEGER, x2), 10);
    Out.Int(x3, 10);
    Out.Real(x4, 10);
    Out.Int(ORD(x5), 10);
    Out.Int(x6.x, 10);
    Out.Ln;
    Out.Int(y0[0], 10);
    IF ~y1[0] THEN  Out.Int(0, 10) END;
    Out.Int(SYSTEM.VAL(INTEGER, y2[0]), 10);
    Out.Int(y3[0], 10);
    Out.Real(y4[0], 10);
    Out.Int(ORD(y5[0]), 10);
    Out.Int(y6[0].x, 10);
    Out.Ln;
    x0 := 8;
    Out.Int(x0, 10);
    x1 := TRUE;
    IF x1 THEN  Out.Int(1, 10) END;
    x2 := {8};
    Out.Int(SYSTEM.VAL(INTEGER, x2), 10);
    x3 := 8;
    Out.Int(x3, 10);
    x4 := 8.0;
    Out.Real(x4, 10);
    x5 := 8X;
    Out.Int(ORD(x5), 10);
    x6.x := 8;
    Out.Int(x6.x, 10);
    Out.Ln;
    y0[0] := 8;
    Out.Int(y0[0], 10);
    y1[0] := TRUE;
    IF y1[0] THEN  Out.Int(1, 10) END;
    y2[0] := {8};
    Out.Int(SYSTEM.VAL(INTEGER, y2[0]), 10);
    y3[0] := 8;
    Out.Int(y3[0], 10);
    y4[0] := 8.0;
    Out.Real(y4[0], 10);
    y5[0] := 8X;
    Out.Int(ORD(y5[0]), 10);
    y6[0].x := 8;
    Out.Int(y6[0].x, 10);
  END P;
BEGIN
  x0 := 0;
  Out.Int(x0, 10);
  x1 := FALSE;
  IF ~x1 THEN  Out.Int(0, 10) END;
  x2 := {};
  Out.Int(SYSTEM.VAL(INTEGER, x2), 10);
  x3 := 0;
  Out.Int(x3, 10);
  x4 := 0.0;
  Out.Real(x4, 10);
  x5 := 0X;
  Out.Int(ORD(x5), 10);
  x6.x := 0;
  Out.Int(x6.x, 10);
  Out.Ln;
  y0[0] := 0;
  Out.Int(y0[0], 10);
  y1[0] := FALSE;
  IF ~y1[0] THEN  Out.Int(0, 10) END;
  y2[0] := {};
  Out.Int(SYSTEM.VAL(INTEGER, y2[0]), 10);
  y3[0] := 0;
  Out.Int(y3[0], 10);
  y4[0] := 0.0;
  Out.Real(y4[0], 10);
  y5[0] := 0X;
  Out.Int(ORD(y5[0]), 10);
  y6[0].x := 0;
  Out.Int(y6[0].x, 10);
  Out.Ln;
  P;
END VarInit.