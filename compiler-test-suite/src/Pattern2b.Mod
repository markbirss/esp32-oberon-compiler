MODULE Pattern2b;
  IMPORT Out, SYSTEM;
  VAR i, j: INTEGER;
      x, y: REAL;
      s, t: SET;
BEGIN 
  x := 1.0+7.0;
  Out.Real(x, 10);
  y := 7.0;
  x := 1.0+y;
  Out.Real(x, 10);
  y := 9.0;
  x := y-1.0;
  Out.Real(x, 10);
  y := 4.0;
  x := y*2.0;
  Out.Real(x, 10);
  y := 16.0;
  x := y/2.0;
  Out.Real(x, 10);
  i := -8;
  Out.Int(i, 10); 
  j := 8;
  i := -j;
  Out.Int(i, 10); 
  x := -8.0;
  Out.Real(x, 10); 
  y := 8.0;
  x := -y;
  Out.Real(x, 10); 
  s := -{1, 3..31};
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
  t := {1, 3..31};
  s := -t;
  Out.Int(SYSTEM.VAL(INTEGER, s), 10);
END Pattern2b.